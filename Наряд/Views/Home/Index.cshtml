<script src="~/Scripts/myJScript/addData.copyString.saveData.js"></script><!--Файл JS в котором будит обработчик нажатия кнопок-->
<script src="~/Scripts/myJScript/workTheTableFacial.js"></script><!--робота с ячейками таблицы Лицевая-->
