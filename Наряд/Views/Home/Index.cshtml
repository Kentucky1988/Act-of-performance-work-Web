@{
    ViewBag.Title = "Index"; //имя вкладки, отображается в закладках
}
<div>
    <div class="container">
        <ul class="nav nav-tabs" role="tablist">
            <li class="active"><a href="#facial"  role="tab" data-toggle="tab">Лицьова сторона</a> </li>
            <li>               <a href="#reverse" role="tab" data-toggle="tab">Зворотня сторона</a> </li>
            <li>               <a href="#print"   role="tab" data-toggle="tab">Ескиз печати</a> </li>
        </ul>
    </div>

    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="facial">  @Html.Partial("_facialPage") </div>
        <div role="tabpanel" class="tab-pane" id="reverse"> @Html.Partial("_reversePage") </div>
        <div role="tabpanel" class="tab-pane" id="print">   @Html.Partial("_print")</div>
    </div>
</div>
<br /><div id="loader"></div> @*индикатор загрузки*@


<script type="text/javascript">
    $(function () {
        $("input[type='text'], select").addClass("form-control");
    });
</script>

<script src="~/Scripts/myJScript/datepicker.languageUK.js"></script> <!--файл с кодом Украинского языка календаря и робота с датами-->
